# Surface area for rectangular prism
# SA = 2lw + 2hw + 2lh
#
# (Very very long-running, currently unsolved)

depth 8

precision 0.1f

pattern add
pattern mul

constant 2.0f

input l
input w
input h

case (1.0f,  1.0f,  1.0f)   => 6.0f
case (1.0f,  2.0f,  3.0f)   => 22.0f
case (20.0f, 10.0f, 100.0f) => 6400.0f
